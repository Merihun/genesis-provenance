# Database
DATABASE_URL=postgresql://user:password@localhost:5432/genesis_provenance

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here-generate-with-openssl-rand-base64-32

# Node Environment
NODE_ENV=development

# AWS S3 (for file uploads) - CONFIGURED
AWS_PROFILE=hosted_storage
AWS_REGION=us-west-2
AWS_BUCKET_NAME=your-bucket-name
AWS_FOLDER_PREFIX=your-prefix/

# Email Service (Resend) - CONFIGURED
RESEND_API_KEY=re_your_api_key_here
EMAIL_FROM="Genesis Provenance <noreply@yourdomain.com>"

# Google Cloud Vision AI - CONFIGURED
GOOGLE_CLOUD_PROJECT_ID=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=./genesis-vision-key.json
GOOGLE_VISION_ENABLED=true

# AI Provider Configuration - Advanced Features
GOOGLE_VISION_ROLLOUT_PERCENTAGE=100
# GOOGLE_VISION_ROLLOUT_PERCENTAGE: Set to 0-100 for gradual rollout (e.g., 50 = 50% of organizations use Google Vision)

AI_DUAL_MODE_ENABLED=false
# AI_DUAL_MODE_ENABLED: Run both Google Vision and Mock AI in parallel for comparison (recommended for testing)

AI_LOG_COMPARISONS=false
# AI_LOG_COMPARISONS: Log detailed comparisons between AI providers (auto-enabled if dual-mode is on)

# AI_PROVIDER_OVERRIDES=orgId1:google-vision,orgId2:mock
# AI_PROVIDER_OVERRIDES: Force specific organizations to use specific providers (comma-separated, format: orgId:provider)

# Future Phase 3+ Variables (Placeholders)
# Stripe (for payments)
# STRIPE_SECRET_KEY=
# STRIPE_PUBLISHABLE_KEY=
# STRIPE_WEBHOOK_SECRET=

# n8n Webhooks (for workflow automation)
# N8N_WEBHOOK_URL=

# AWS Rekognition (Alternative AI Provider) - OPTIONAL
# AWS_REKOGNITION_ENABLED=false
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_REKOGNITION_REGION=us-east-1

# Custom ML & Image Preprocessing - OPTIONAL
# AI_CUSTOM_ML_ENABLED=true
# AI_PREPROCESS_IMAGES=true
# AI_MAX_IMAGE_SIZE=2048
